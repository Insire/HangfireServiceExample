<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RuntimeIdentifier>win7-x64</RuntimeIdentifier>
	</PropertyGroup>

	<ItemGroup>
		<ProjectCapability Include="DotNetCoreWeb" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="AutoRegisterInject" Version="1.2.1" />

		<PackageReference Include="Hangfire" Version="1.7.33" />
		<PackageReference Include="Hangfire.Core" Version="1.7.33" />
		<PackageReference Include="Hangfire.InMemory" Version="0.3.6" />

		<PackageReference Include="Microsoft.Extensions.Configuration.CommandLine" Version="7.0.0" />
		<PackageReference Include="Microsoft.Extensions.Hosting" Version="7.0.1" />
		<PackageReference Include="Microsoft.Extensions.Hosting.WindowsServices" Version="7.0.0" />
	</ItemGroup>

</Project>
